{
  "version": 2,
  "name": "yt-podcast-api",
  "builds": [
    {
      "src": "dist/src/main.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js"
    }
  ],
  "env": {
    "DB_HOST": "db_host",
    "DB_USER": "db_user",
    "DB_PASS": "db_pass",
    "DB_NAME": "db_name"
  }
}